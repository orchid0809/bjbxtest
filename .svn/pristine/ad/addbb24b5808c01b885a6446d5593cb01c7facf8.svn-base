.pic-area .list li{ width: 66px; height: 66px;}
.pic-min { width: 64px; height: 64px; border: 1px solid #e4e4e4; text-align: center; overflow: hidden; }
.pic-area .list .on .pic-min{border: 1px solid #d43225;}
.pic-min img { padding: 2px; max-width: 60px; max-height: 60px; vertical-align: middle; overflow: hidden; }
* html .pic-min img { width: expression(this.width > 60 && this.width > this.height ? 60:auto); height: expression(this.height > 60 ? 60:auto); }